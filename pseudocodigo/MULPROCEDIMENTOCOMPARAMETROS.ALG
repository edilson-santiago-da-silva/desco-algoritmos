Algoritmo "mulProcedimentoComParametros"

procedimento Mult(x,y:inteiro)

Var
// Seção de Declarações das variáveis
aux: inteiro
Inicio
      aux <- x * y
      res <- aux
      escreva(res)
Fimprocedimento

Var
res, n1, n2: inteiro
Inicio
      leia(n1)
      leia(n2)
      mult(n1,n2)
Fimalgoritmo